<div class="h_centred">
    <table class="even-odd-list">
        <tr>
            <th><span class="sorting">ID</span></th>
            <th><span class="sorting">Имя</span></th>
            <th><span class="sorting">Телефон</span></th>
            <th><span class="sorting_desc">Дата добавления<span></th>
        </tr>
        [%START phones%]
        <tr>
            <td>[% id %]</td>
            <td>[% name %]</td>
            <td>[% phone %]</td>
            <td>[% created %]</td>
        </tr>
        [%END phones%]
    </table>
    <div class="pagination">
        [%INCLUDE pagination%]
    </div>
</div>
